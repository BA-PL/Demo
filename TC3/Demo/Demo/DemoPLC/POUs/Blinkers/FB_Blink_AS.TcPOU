  <POU Name="FB_Blink_AS" Id="{308ba326-d075-48da-b682-df9e6ba1d528}" SpecialFunc="None">
    <Declaration><![CDATA[(********************************************************************************************)
(*							Beckhoff Automation Poland										*)
(*								FB_Blink_AS	 v1.1											*)
(*									9.11.2007												*)
(*						Blok funkcyjny zegara asynchronicznego								*)
