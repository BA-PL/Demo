FUNCTION_BLOCK FB_Blink_AS
VAR_INPUT
	T1										:	TIME;				(*Czas, gdy wyjście przyjmuje wartość TRUE*)
	T0										:	TIME;				(*Czas, gdy wyjście przyjmuje wartość FALSE*)
END_VAR
VAR_OUTPUT
	Q										:	BOOL;				(*Wyjscie*)
END_VAR
VAR
	ton1									:	TON;
	ton2									:	TON;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[ton1(IN:= NOT ton2.Q, PT:=T0);
Q:=ton1.Q;
ton2(IN:=ton1.Q , PT:=T1);]]></ST>
    </Implementation>
  </POU>
